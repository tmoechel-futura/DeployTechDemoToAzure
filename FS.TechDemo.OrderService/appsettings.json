{
  "AllowedHosts": "*",
  "MessageBroker": {
    "Broker": {
      "DelayedRedelivery": {
        "FirstIntervalMinutes": 5,
        "SecondIntervalMinutes": 10,
        "ThirdIntervalMinutes": 20
      },
      "MessageRetry": {
        "Limit": 3,
        "InitialIntervalMilliseconds": 500,
        "IntervalIncrementMilliseconds": 500
      },
      "RabbitMq": {
        "Host": "localhost",
        "VirtualHost": "/",
        "Username": "rabbitmq-user",
        "Password": "rabbitmq-password"
      }
    }
  }
}
